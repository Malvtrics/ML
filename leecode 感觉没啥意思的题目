932漂亮数组
class Solution:
    def beautifulArray(self, N: int) -> List[int]:
        def _helper(nob_list):
            if len(nob_list) <= 2:
                return nob_list

            b1 = _helper(nob_list[::2])
            b2 = _helper(nob_list[1::2])
            return b1 + b2

        n_list = list(range(1, N+1))
        beatiful_list = _helper(n_list)
        return beatiful_list
